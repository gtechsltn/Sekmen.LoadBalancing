        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ServerRole": "Subscriber",
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "Global": {
        "Id": "2503d3f3-ea77-4923-8f15-9152b108ce95"
      },
      "WebRouting": {
        "DisableRedirectUrlTracking": true
      }
    }
  }
}